  pages      = {481--505},
  shorttitle = {Rigidly {Applied} {Rules}?},
  title      = {Rigidly {Applied} {Rules}? {Revisiting} {Inflexibility} in {Obsessive} {Compulsive} {Disorder} {Using} {Multilevel} {Meta}-{Analysis}},
