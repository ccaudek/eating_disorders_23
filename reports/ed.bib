  doi        = {10.1073/pnas.95.23.13755},
  file       = {Full Text PDF:/Users/corrado/Zotero/storage/8SEJ9THZ/Milinski and Wedekind - 1998 - Working memory constrains human cooperation in the.pdf:application/pdf;Snapshot:/Users/corrado/Zotero/storage/8KLT4T4Q/13755.html:text/html},
  issn       = {0027-8424, 1091-6490},
  journal    = {Proceedings of the National Academy of Sciences},
  language   = {en},
  month      = nov,
  number     = {23},
  pages      = {13755--13758},
  pmid       = {9811873},
  title      = {Working memory constrains human cooperation in the {Prisoner}'s {Dilemma}},
  url        = {https://www.pnas.org/content/95/23/13755},
  urldate    = {2019-01-14},
