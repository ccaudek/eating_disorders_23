}

@article{coles_not_2003,
  abstract   = {Individuals with obsessive--compulsive disorder (OCD) frequently report uncomfortable sensations of things not being just right (``not just right experiences''; NJREs) and a need to ritualize until they quiet these sensations. However, very little work has been conducted to empirically examine the nature and characteristics of this phenomenon. In this paper, we present two studies that systematically examine NJREs in large undergraduate samples. In Study 1, NJREs were shown to be related to both OCD features and maladaptive domains of perfectionism. In Study 2, NJREs were again significantly related to OCD features. Further, NJREs were shown to have particularly strong relationships with specific OCD symptom clusters (e.g., checking, ordering). Finally, NJREs were significantly more strongly correlated with features of OCD than symptoms from other domains of psychopathology (e.g., social anxiety, trait anxiety, worry, depression). General conclusions are drawn, and future directions are discussed.},
  author     = {Coles, Meredith E. and Frost, Randy O. and Heimberg, Richard G. and Rh{\'e}aume, Jos{\'e}e},
  doi        = {10.1016/S0005-7967(02)00044-X},
  file       = {ScienceDirect Full Text PDF:/Users/corrado/Zotero/storage/PBQHGJSW/Coles et al. - 2003 - ``Not just right experiences'' perfectionism, obses.pdf:application/pdf;ScienceDirect Snapshot:/Users/corrado/Zotero/storage/YDAJWWM8/S000579670200044X.html:text/html},
  issn       = {0005-7967},
  journal    = {Behaviour Research and Therapy},
  keywords   = {Not just right experiences, Obsessive-compulsive features, Perfectionism},
  month      = jun,
  number     = {6},
  pages      = {681--700},
  shorttitle = {``{Not} just right experiences''},
  title      = {``{Not} just right experiences'': perfectionism, obsessive--compulsive features and general psychopathology},
  url        = {http://www.sciencedirect.com/science/article/pii/S000579670200044X},
  urldate    = {2019-01-11},
  volume     = {41},
  year       = {2003},
  bdsk-url-1 = {http://www.sciencedirect.com/science/article/pii/S000579670200044X},
  bdsk-url-2 = {https://doi.org/10.1016/S0005-7967(02)00044-X}
