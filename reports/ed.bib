  title         = {The {Eating} {Attitudes} {Test}: psychometric features and clinical correlates},
  volume        = {12},
  year          = {1982},
  bdsk-url-1    = {https://www.cambridge.org/core/journals/psychological-medicine/article/eating-attitudes-test-psychometric-features-and-clinical-correlates/F24C1A1C33ADEDA0023B6053C6AA1EF5},
