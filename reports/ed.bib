  shorttitle = {The {Endophenotype} {Concept} in {Psychiatry}},
  title      = {The {Endophenotype} {Concept} in {Psychiatry}: {Etymology} and {Strategic} {Intentions}},
  url        = {https://ajp.psychiatryonline.org/doi/full/10.1176/appi.ajp.160.4.636},
