  number     = {7186},
  pages      = {765--768},
  pmid       = {10082698},
  shorttitle = {Onset of adolescent eating disorders},
  title      = {Onset of adolescent eating disorders: population based cohort study over 3 years},
  url        = {https://www.bmj.com/content/318/7186/765},
  urldate    = {2019-12-06},
